{
  "name": "git_test",
  "version": "1.0.0",
  "description": "first time using nodejs and npm",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss ",
    "clean": "rimraf dist",
    "usemin": "usemin about.html -d dist -o dist/about.html --htmlmin true --rmlr truenp",
    "build": "npm run clean && npm run scss && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/cgutierr3z/git_test.git"
  },
  "author": "cgutierr3z",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://bitbucket.org/cgutierr3z/git_test/issues"
  },
  "homepage": "https://bitbucket.org/cgutierr3z/git_test#readme",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.4.0",
    "node-sass": "^4.9.3",
    "onchange": "^4.1.0",
    "rimraf": "^2.6.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.2",
    "concurrently": "^4.0.1",
    "jquery": "^3.3.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
}

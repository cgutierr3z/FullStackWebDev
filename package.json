{
  "name": "git_test",
  "version": "1.0.0",
  "description": "first time using nodejs and npm",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss ",
    "clean": "rimraf dist",
    "usemin": "usemin index.html -d dist -o dist/index.html --htmlmin true --rmlr true && usemin about.html -d dist -o dist/about.html --htmlmin true --rmlr true && usemin pricing.html -d dist -o dist/pricing.html --htmlmin true --rmlr true && usemin terms.html -d dist -o dist/terms.html --htmlmin true --rmlr true && usemin job.html -d dist -o dist/job.html --htmlmin true --rmlr true && usemin contact.html -d dist -o dist/contact.html --htmlmin true --rmlr true",
    "build": "npm run clean && npm run scss && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/cgutierr3z/git_test.git"
  },
  "author": "cgutierr3z",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://bitbucket.org/cgutierr3z/git_test/issues"
  },
  "homepage": "https://bitbucket.org/cgutierr3z/git_test#readme",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "grunt": "^1.0.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "htmlmin": "^0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.4.0",
    "node-sass": "^4.9.3",
    "onchange": "^4.1.0",
    "rimraf": "^2.6.2",
    "ruby": "^0.6.1",
    "sass": "^1.49.9",
    "time-grunt": "^1.4.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.2",
    "bufferutil": "^4.0.1",
    "concurrently": "^4.0.1",
    "jquery": "^3.3.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "utf-8-validate": "^5.0.2"
  }
}
